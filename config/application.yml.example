lims_host: 192.168.0.101
lims_prefix: api/v1
lims_protocol: http
lims_port: 3010
lims_username: bht_emr_api_1
lims_password: 123456789
lims_default_user: 'admin'
lims_default_password: 'knock_knock'
lims_partner: 'BHT'
lims_app_name: 'bht_emr_api'


# Configuration for DDE
dde:
  url: http://localhost:5000

  # This section has DDE username and password for various applications.
  # The application's name must be the same as a program name in the
  # program's table <em>but in lowercase</em>.
  hiv program:
    username: admin
    password: bht.dde3!

  tb program:
    username: admin
    password: bht.dde3!

# CouchDB configuration
#
# This is optional, it is required only if you intend to sync data with
# the RDS (ie if you intend to `bin/rds_push`)
couchdb:
  local:
    protocol: http
    host: 127.0.0.1
    port: 5984
    database: rds
    username: admin
    password: password

  master:
    protocol: http
    host: 127.0.0.1
    port: 5984
    database: rds
    username: admin
    password: password

# Required for `bin/rds_push` when targeting databases that don't
# have encounter_id (or is always nil) in encounters table.
program_name: hiv program
